<p id="notice"><%= notice %></p>

<h1>Listing Residentes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Telefono</th>
      <th>Fecha nacimiento</th>
      <th>Identificacion</th>
      <th>Apartamento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @residentes.each do |residente| %>
      <tr>
        <td><%= residente.nombre %></td>
        <td><%= residente.apellido %></td>
        <td><%= residente.telefono %></td>
        <td><%= residente.fecha_nacimiento %></td>
        <td><%= residente.identificacion %></td>
        <td><%= residente.apartamento %></td>
        <td><%= link_to 'Show', residente %></td>
        <td><%= link_to 'Edit', edit_residente_path(residente) %></td>
        <td><%= link_to 'Destroy', residente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Residente', new_residente_path %>
